#include<iostream>
using namespace std;
int main() {
	int n;
	cin>>n;
	int a[n];
	for(int i=0;i<n;i++)
		cin>>a[i];
	int k;
	cin>>k;
	int lb=0;
	int ub=n-1;
	while(lb<=ub){
		int mid=(lb+ub)/2;
		if(a[mid]==k){
            cout<<mid<<"\n";
            return 0;
        }
		if(a[mid]>a[lb]){
            if(a[lb]<=k&&a[mid]>k)
                ub=mid-1;
            else
                lb=mid+1;
		}
		else{
            if(a[mid]<k&&a[ub]>=k)
                lb=mid+1;
            else
                ub=mid-1;
	}
	}
	cout<<-1;
	return 0;
}
